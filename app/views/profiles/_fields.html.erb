<div>
  <div class="form-group">
    <%= f.label :name %> 
    <%= f.text_field :name %>
    <small style="color:red;">* Required</small>
  </div>
  <div class="form-group">
    <%= f.label :url %>
    <%= f.text_field :url %>
  </div>
  <div class="form-group">
    <span class="span1"><i class='fa fa-facebook-square fa-4x'></i></span>
    <%= f.label :facebook %>
    <%= f.text_field :facebook %>
  </div>
  <div class="form-group">
    <span class="span1"><i class='fa fa-twitter-square fa-4x'></i></span>
    <%= f.label :twitter %>
    <%= f.text_field :twitter %>
  </div>
  <div class="form-group">
    <span class="span1"><i class='fa fa-instagram fa-4x'></i></span>
    <%= f.label :instagram %>
    <%= f.text_field :instagram %>
  </div>
  <div class="form-group">
    <span class="span1"><i class='fa fa-linkedin-square fa-4x'></i></span>
    <%= f.label :linkedIn %>
    <%= f.text_field :linkedin %>
  </div>
  <div class="form-group">
    <span class="span1"><i class='fa fa-volume-up fa-4x'></i></span>
    <%= f.label :audio %>
    <%= f.text_field :audio %>
  </div>
  <div class="form-group">
    <span class="span1"><i class='fa fa-play-circle-o fa-4x'></i></span>
    <%= f.label :video %>
    <%= f.text_field :video %>
  </div>
  <div class="form-group">
  <span class="span1"><i class='fa fa-play-circle-o fa-4x'></i></span>
    <%= f.label :tag_list, "Tags (separated by commas)" %><br />
    <%= f.text_field :tag_list, options = {'value' => @profile.tag_list.to_s, 'data-role' => 'tagsinput'} %>
  </div>
  <!-- <span class="span7 profile_edit">
    <span class="span1"><i class='fa fa-google-plus-square fa-4x'></i></span>
    <%#= f.label :googleplus %>
    <%#= f.text_field :googleplus %>
  </span> -->
  <!-- <span class="span7 offset1 profile_edit">
    <%#= f.label :join %>
    <%#= f.text_field :join %>
  </span>
  <span class="span7 offset1 profile_edit">
    <%#= f.label :donate %>
    <%#= f.text_field :donate %>
  </span> -->
</div>



