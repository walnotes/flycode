<h1 class="text-left">Create a Profile</h1>

<div class="row">
  <div class="span6">
	<%= form_for(@profile) do |f| %>
	  <%= render 'shared/error_messages', :object => f.object %>
	  <%= render 'fields', :f => f %>
	  <div class="actions">
	    <%= f.submit "Submit", class: 'btn btn-large btn-primary offset4' %>
	  </div>
	<% end %>
 </div>
</div>
